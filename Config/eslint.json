{
	"env": {
		"browser": true,
		"node": true,
		"mocha": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"ignorePatterns": ["Interface/Addons/**"],
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double", { "avoidEscape": true }],
		"semi": ["error", "always"],
		"no-redeclare": ["error", { "builtinGlobals": false }],
		"no-unused-vars": "off"
	},
	"globals": {
		"assertTrue": "writable",
		"assertDeepEquals": "writable",
		"assertFalse": "writable",
		"C_Addons": "writable",
		"C_Bitmap": "writable",
		"C_Decoding": "writable",
		"C_EventSystem": "writable",
		"C_FileSystem": "writable",
		"C_Keybindings": "writable",
		"C_Locales": "writable",
		"C_Logging": "writable",
		"C_Macro": "writable",
		"C_Navigation": "writable",
		"C_Networking": "writable",
		"C_Profiling": "writable",
		"C_Rendering": "writable",
		"C_Resources": "writable",
		"C_Settings": "writable",
		"C_System": "writable",
		"C_Testing": "writable",
		"C_Validation": "writable",
		"C_WebGL": "writable",
		"BinaryReader": "writable",
		"Bitmap": "writable",
		"Button": "writable",
		"Canvas": "writable",
		"Channels": "writable",
		"CheckButton": "writable",
		"EditBox": "writable",
		"Enum": "writable",
		"FieldSet": "writable",
		"FontString": "writable",
		"Frame": "writable",
		"Label": "writable",
		"Legend": "writable",
		"OptionsGroup": "writable",
		"Paragraph": "writable",
		"ScriptObject": "writable",
		"ScrollFrame": "writable",
		"Table": "writable",
		"UIParent": "writable",
		"ViewportContainer": "writable",
		"WebClient": "writable",
		"Widget": "writable",
		"BABYLON": "readable",
		"BITMAP": "writable",
		"CONSTANTS": "writable",
		"DEBUG": "writable",
		"printf": "writable",
		"INFO": "writable",
		"JOI": "writable",
		"L": "writable",
		"MESSAGEPACK": "writable",
		"NOTICE": "writable",
		"WARNING": "writable",
		"dump": "writable",
		"WEBCLIENT_ADDONS_DIR": "writable",
		"WEBCLIENT_ASSETS_DIR": "writable",
		"WEBCLIENT_EXPORTS_DIR": "writable",
		"WEBCLIENT_INTERFACE_DIR": "writable",
		"WEBCLIENT_LOCALES_DIR": "writable",
		"WEBCLIENT_ROOT_DIR": "writable",
		"WEBCLIENT_SETTINGS_DIR": "writable"
	}
}
