{
	"name": "revival-webclient",
	"version": "0.21.0619",
	"description": "Browser-based client capable of rendering networked multiplayer games using modern web technologies.",
	"main": "main.js",
	"homepage": "https://revivalengine.github.io/",
	"dependencies": {
		"@msgpack/msgpack": "2.5.1",
		"babylonjs": "4.2.0",
		"babylonjs-gui": "4.2.0",
		"babylonjs-loaders": "4.2.0",
		"babylonjs-materials": "4.2.0",
		"babylonjs-serializers": "4.2.0",
		"bmp-js": "^0.1.0",
		"joi": "17.4.1"
	},
	"devDependencies": {
		"electron": "13.1.7",
		"electron-builder": "22.11.7",
		"electron-mocha": "10.1.0",
		"eslint": "7.31.0",
		"mocha": "9.0.2"
	},
	"scripts": {
		"test": "electron-mocha --renderer --url index.html Tests",
		"test-main": "electron-mocha Tests/Electron",
		"all-tests": "electron-mocha Tests/Electron && electron-mocha --renderer --url index.html Tests",
		"checks": "npx eslint --config Config/eslint.json .",
		"start": "electron .",
		"oom": "electron --js-flags='--max-old-space-size=10192' .",
		"dev": "electron --js-flags=--max-old-space-size=500 . ",
		"pack": "electron-builder --dir --config Config/electron-builder.json",
		"make": "electron-builder --config Config/electron-builder.json",
		"make-win64": "electron-builder --windows nsis:x64 --config Config/electron-builder.json",
		"make-win64-portable": "electron-builder --windows portable:x64 --config Config/electron-builder.json",
		"make-ubuntu": "electron-builder --linux deb --config Config/electron-builder.json",
		"make-all": "electron-builder -mwl --config Config/electron-builder.json",
		"make-windows-linux": "electron-builder -wl --config Config/electron-builder.json"
	},
	"keywords": [],
	"author": "RDW",
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/RevivalEngine/WebClient.git"
	}
}